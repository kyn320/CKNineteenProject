				float temp_output_28_0 = pow( ( 1.0 - uv_MainTex.y ) , _Mask_Powe );
				float2 appendResult13 = (float2(0.0 , _Main_Vpanner));
				float2 panner12 = ( 1.0 * _Time.y * appendResult13 + uv_MainTex);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = _TintColor.rgb;
